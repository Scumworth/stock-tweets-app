{
  "name": "stock-tweets-app-server",
  "version": "0.1.0",
  "engines": {
    "node": ">10.0.0"
  },
  "private": true,
  "scripts": {
    "server": "cd server && node ./bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postBuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {   
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Scumworth/stock-tweets-app.git"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
